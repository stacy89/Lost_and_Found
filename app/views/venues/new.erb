<form id="form-center" action="/venues" method="post">
	<input type="text" name="venue[username]" placeholder="username:"><br><br>
	<input type="text" name="venue[first_name]" placeholder="first_name:"><br><br>
	<input type="text" name="venue[last_name]" placeholder="last_name:"><br><br>
	<input type="text" name="venue[name]" placeholder="business name:"><br><br>
	<input type="text" name="venue[location]" placeholder="location:"><br><br>
	<input type="text" name="venue[email]" placeholder="email:"><br><br>
