<h2>For Users:</h2><br>

<form id="form-center" enctype="multipart/form-data" action="/users" method="POST">

	<label>Username:</label>
	<input type="text" name="user[username]"><br><br>
	
	<label>First name:</label>
	<input type="text" name="user[first_name]"><br><br>
	
	<label>Last name:</label>
	<input type="text" name="user[last_name]"><br><br>
	
	<label>Location:</label>
	<input type="text" name="user[location]"><br><br>
	
	<label>Email:</label>
	<input type="text" name="user[email]"><br><br>

	<label>Password:</label>
	<input type="password" name="user[password]"><br><br>

	<input type="file" name="user[image]">

	<input type="submit" value="submit">
</form><br><br><br>

